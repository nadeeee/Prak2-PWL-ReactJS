ton>\n      </form>\n      <div className={classes.commentarea}>\n          <h4 className={classes.jumlah}>{countComment} Comments</h4>\n          {users.map((user) => (\n            <div className={classes.comment}>\n              <div className={classes.commentUser}>\n                <div className={classes.userInfo}>\n                  <h4>{user.name}</h4>\n                  <p>{user.email}</p>\n                </div>\n              </div>\n              <p>{user.comment}</p>\n            </div>\n          ))}\n        </div>\n   </div>\n   </div>\n  \n  )\n   \n  // )\n}\n\nexport default Contact\n","/Applications/MAMP/htdocs/Prak2-PWL-ReactJS/src/components/Header/Header.js",[],"/Applications/MAMP/htdocs/Prak2-PWL-ReactJS/src/components/Projects/Projects.js",[],"/Applications/MAMP/htdocs/Prak2-PWL-ReactJS/src/components/ScrollToTop/ScrollToTop.js",[],"/Applications/MAMP/htdocs/Prak2-PWL-ReactJS/src/components/Footer/Footer.js",[],"/Applications/MAMP/htdocs/Prak2-PWL-ReactJS/src/components/Contact/contact.jsx",[],"/Applications/MAMP/htdocs/Prak2-PWL-ReactJS/src/portfolio.js",[],"/Applications/MAMP/htdocs/Prak2-PWL-ReactJS/src/components/ProjectContainer/ProjectContainer.js",[],"/Applications/MAMP/htdocs/Prak2-PWL-ReactJS/src/components/Navbar/Navbar.js",[],"/Applications/MAMP/htdocs/ReactJS_PRAK/Untitled/src/index.js",[],"/Applications/MAMP/htdocs/ReactJS_PRAK/Untitled/src/contexts/theme.js",[],"/Applications/MAMP/htdocs/ReactJS_PRAK/Untitled/src/App.js",[],"/Applications/MAMP/htdocs/ReactJS_PRAK/Untitled/src/components/Header/Header.js",[],"/Applications/MAMP/htdocs/ReactJS_PRAK/Untitled/src/components/Contact/Contact.js",[],"/Applications/MAMP/htdocs/ReactJS_PRAK/Untitled/src/components/About/About.js",[],"/Applications/MAMP/htdocs/ReactJS_PRAK/Untitled/src/components/Skills/Skills.js",[],"/Applications/MAMP/htdocs/ReactJS_PRAK/Untitled/src/components/Projects/Projects.js",[],"/Applications/MAMP/htdocs/ReactJS_PRAK/Untitled/src/components/ScrollToTop/ScrollToT